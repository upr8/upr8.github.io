{
  "name": "personal-web-site",
  "version": "1.0.0",
  "private": false,
  "description": "A personal website.",
  "author": "",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "gh-pages-action": "gh-pages -d public -b release -x -r https://$GH_TOKEN@github.com/upr8/upr8.github.io.git --remove '**/*'",
    "deploy-action": "npm run build && npm run gh-pages-action",
    "deploy": "npm run build && gh-pages -d public -b release --remove '**/*'",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "biome:check": "biome check .",
    "biome:check:apply": "biome check . --write",
    "biome:check:apply:unsafe": "biome check . --apply-unsafe",
    "biome:format": "biome format . --write",
    "fix": "npm run biome:format && npm run biome:check:apply",
    "fix:unsafe": "npm run biome:format && npm run typecheck && npm run biome:check:apply:unsafe"
  },
  "license": "MIT",
  "dependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "gatsby": "^5.14.0",
    "gatsby-plugin-feed": "^5.13.1",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-mdx": "^5.14.0",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-remark-autolink-headers": "^6.14.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-json": "^5.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.20",
    "gatsby-plugin-postcss": "^6.14.0",
    "gh-pages": "^6.2.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
