{
  "name": "personal-web-site",
  "version": "1.0.0",
  "private": false,
  "description": "A personal website.",
  "author": "",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "gh-pages-action": "gh-pages -d public -b release -x -r https://$GH_TOKEN@github.com/yruc/yruc.github.io.git --remove '**/*'",
    "deploy-action": "npm run build && npm run gh-pages-action",
    "deploy": "npm run build && gh-pages -d public -b release --remove '**/*'",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "rome:check": "rome check .",
    "rome:check:apply": "rome check . --apply",
    "rome:check:apply:unsafe": "rome check . --apply-unsafe",
    "rome:format": "rome format . --write",
    "fix": "npm run rome:format && npm run rome:check:apply",
    "fix:unsafe": "npm run rome:format && npm run typecheck && npm run rome:check:apply:unsafe"
  },
  "license": "MIT",
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "gatsby": "^5.10.0",
    "gatsby-plugin-feed": "^5.10.0",
    "gatsby-plugin-image": "^3.10.0",
    "gatsby-plugin-mdx": "^5.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-sitemap": "^6.10.0",
    "gatsby-remark-images": "^7.10.0",
    "gatsby-source-filesystem": "^5.10.0",
    "gatsby-transformer-sharp": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.5",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@types/react-helmet": "^6.1.6",
    "@types/react-syntax-highlighter": "^15.5.7",
    "autoprefixer": "^10.4.14",
    "gatsby-plugin-postcss": "^6.10.0",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.23",
    "rome": "12.1.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1"
  }
}
