{
  "name": "personal-web-site",
  "version": "1.0.0",
  "private": false,
  "description": "A personal website.",
  "author": "",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "gh-pages-action": "gh-pages -d public -b release -x -r https://$GH_TOKEN@github.com/upr8/upr8.github.io.git --remove '**/*'",
    "deploy-action": "npm run build && npm run gh-pages-action",
    "deploy": "npm run build && gh-pages -d public -b release --remove '**/*'",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "rome:check": "rome check .",
    "rome:check:apply": "rome check . --apply",
    "rome:check:apply:unsafe": "rome check . --apply-unsafe",
    "rome:format": "rome format . --write",
    "fix": "npm run rome:format && npm run rome:check:apply",
    "fix:unsafe": "npm run rome:format && npm run typecheck && npm run rome:check:apply:unsafe"
  },
  "license": "MIT",
  "dependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "gatsby": "^5.12.4",
    "gatsby-plugin-feed": "^5.12.0",
    "gatsby-plugin-google-gtag": "^5.12.0",
    "gatsby-plugin-image": "^3.12.0",
    "gatsby-plugin-manifest": "^5.12.0",
    "gatsby-plugin-mdx": "^5.12.0",
    "gatsby-plugin-offline": "^6.12.0",
    "gatsby-plugin-sharp": "^5.12.0",
    "gatsby-plugin-sitemap": "^6.12.0",
    "gatsby-remark-autolink-headers": "^6.12.0",
    "gatsby-remark-copy-linked-files": "^6.12.0",
    "gatsby-remark-images": "^7.12.0",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-sharp": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/react-syntax-highlighter": "^15.5.7",
    "autoprefixer": "^10.4.15",
    "gatsby-plugin-postcss": "^6.12.0",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.29",
    "rome": "12.1.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.1"
  }
}
