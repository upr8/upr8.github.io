{
  "name": "personal-web-site",
  "version": "1.0.0",
  "private": false,
  "description": "A personal website.",
  "author": "",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "gh-pages-action": "gh-pages -d public -b release --remove '**/*'",
    "deploy-action": "npm run build && npm run gh-pages-action",
    "deploy": "npm run build && gh-pages -d public -b release --remove '**/*'",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "biome:check": "biome check .",
    "biome:check:apply": "biome check . --write",
    "biome:check:apply:unsafe": "biome check . --apply-unsafe",
    "biome:format": "biome format . --write",
    "fix": "npm run biome:format && npm run biome:check:apply",
    "fix:unsafe": "npm run biome:format && npm run typecheck && npm run biome:check:apply:unsafe"
  },
  "license": "MIT",
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "gatsby": "^5.15.0",
    "gatsby-plugin-feed": "^5.15.0",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-mdx": "^5.15.0",
    "gatsby-plugin-offline": "^6.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-remark-autolink-headers": "^6.15.0",
    "gatsby-remark-copy-linked-files": "^6.15.0",
    "gatsby-remark-images": "^7.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-json": "^5.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet": "^6.1.11",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.22",
    "gatsby-plugin-postcss": "^6.15.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0"
  }
}
