{
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "[json]": {
        "editor.defaultFormatter": "rome.rome"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "rome.rome"
    },
    "[javascript]": {
        "editor.defaultFormatter": "rome.rome"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "rome.rome"
    },
    "[typescript]": {
        "editor.defaultFormatter": "rome.rome"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "rome.rome"
    }
}
