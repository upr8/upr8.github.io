{
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "files.associations": {
        "*.css": "postcss"
    },
    "[json]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "cSpell.enabled": true,
    "cSpell.words": [
        "biomejs",
        "bradlc",
        "codepaths",
        "csstools",
        "devcontainer",
        "devcontainers",
        "eamodio",
        "frontmatter",
        "gtag",
        "heroicon",
        "heybourn",
        "localstorage",
        "oderwat",
        "pflannery",
        "ppas",
        "shortcodes",
        "slugtag",
        "tailwindcss",
        "tsbuildinfo",
        "typecheck",
        "Typegen",
        "unifiedjs",
        "versionlens",
        "vtop"
    ]
}
